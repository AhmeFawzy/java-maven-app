pipeline {
    agent any      
    stages {
        stage("test") {            
            steps {
                script {
                    echo " testing the integration"                                    
                }
            }
        }
        stage("build jar") {
            steps {
                script {
                    echo " jar"
            
                }
            }
        }
        stage("build image") {
            steps {
                script {
                    echo " image"
                 
                }
            }
        }
        stage("deploy") {
            steps {
                script {
                    echo "deploying"
              
                }
            }
        }
   }   
}
  
