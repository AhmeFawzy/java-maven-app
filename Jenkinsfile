#!usr/bin/env groovy
pipeline {
    agent any      
    stages {
        stage("test") {            
            steps {
                script {
                    echo "i'm just wondering, when i will pass all that shit"                                    
                }
            }
        }
        stage("build jar") {
            steps {
                script {
                    echo " jar bar disco"
            
                }
            }
        }
        stage("build image") {
            steps {
                script {
                    echo " image loves you so much"
                 
                }
            }
        }
        stage("deploy") {
            steps {
                script {
                    echo "deploying"
              
                }
            }
        }
   }   
}
  
